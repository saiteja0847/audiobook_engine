    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "narration",
      "emotion": "tense",
      "start_paragraph": 71,
      "end_paragraph": 71
    },
    "emotion_prompt": "neutral"
  },
  {
    "chunk_id": 44,
    "text": "It\u2019s the first time Mom and I have been alone in months.",
