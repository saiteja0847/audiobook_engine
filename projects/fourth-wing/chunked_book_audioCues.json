    "metadata": {
      "type": "dialogue",
      "emotion": "disbelief",
      "character_relationship": "sisterly",
      "start_paragraph": 86,
      "end_paragraph": 86
    },
    "emotion_prompt": "whispering"
  },
  {
    "chunk_id": 55,
    "text": "Mira says from the center of the hallway, right between where two guards are positioned.",
    "speaker": "NARRATOR",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
