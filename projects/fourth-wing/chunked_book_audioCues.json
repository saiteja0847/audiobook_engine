    "metadata": {
      "type": "narration",
      "emotion": "neutral",
      "start_paragraph": 86,
      "end_paragraph": 86
    },
    "emotion_prompt": "neutral"
  },
  {
    "chunk_id": 56,
    "text": "[whispering] They\u2019ll tell her you said that.",
    "speaker": "Violet Sorrengail",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "warning",
      "character_relationship": "sisterly",
      "start_paragraph": 87,
      "end_paragraph": 87
    },
    "emotion_prompt": "whispering"
  },
  {
    "chunk_id": 57,
    "text": "[gritting teeth] Like they don\u2019t already know.",
