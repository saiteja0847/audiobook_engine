    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "disappointed",
      "character_relationship": "mother_daughter",
      "start_paragraph": 39,
      "end_paragraph": 39
    },
    "emotion_prompt": "disappointed"
  },
  {
    "chunk_id": 25,
    "text": "I told him not to keep you in that library.",
