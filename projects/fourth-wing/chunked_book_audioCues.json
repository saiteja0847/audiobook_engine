    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "critical",
      "character_relationship": "mother_daughter",
      "start_paragraph": 38,
      "end_paragraph": 38
    },
    "emotion_prompt": "critical"
  },
  {
    "chunk_id": 24,
    "text": "It's like that fever stole all your coloring along with your strength.",
