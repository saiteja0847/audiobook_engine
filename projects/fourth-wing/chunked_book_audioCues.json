    "speaker": "Mira Sorrengail",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "desperate",
      "character_relationship": "daughter_mother",
      "start_paragraph": 56,
      "end_paragraph": 56
    },
    "emotion_prompt": "shouting"
  },
