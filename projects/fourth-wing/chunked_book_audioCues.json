    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "annoyed",
      "character_relationship": "mother_daughter",
      "start_paragraph": 40,
      "end_paragraph": 40
    },
    "emotion_prompt": "bitter"
  },
  {
