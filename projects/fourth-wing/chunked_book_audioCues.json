      "start_paragraph": 96,
      "end_paragraph": 96
    },
    "emotion_prompt": "serious"
  },
  {
    "chunk_id": 60,
    "text": "I shake my head.",
    "speaker": "NARRATOR",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
