    "metadata": {
      "type": "dialogue",
      "emotion": "annoyed",
      "character_relationship": "sisterly",
      "start_paragraph": 87,
      "end_paragraph": 87
    },
    "emotion_prompt": "annoyed"
  },
  {
    "chunk_id": 58,
    "text": "[sighs] She had my entire life boxed.",
    "speaker": "NARRATOR",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "internal_monologue",
      "emotion": "sad",
      "start_paragraph": 91,
      "end_paragraph": 91
    },
    "emotion_prompt": "sad"
  },
  {
    "chunk_id": 59,
    "text": "Can you use a sword?",
    "speaker": "Mira Sorrengail",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
