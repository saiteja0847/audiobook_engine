    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "angry",
      "character_relationship": "daughter_mother",
      "start_paragraph": 50,
      "end_paragraph": 50
    },
    "emotion_prompt": "passionate"
  },
  {
    "chunk_id": 35,
    "text": "I loved your father, but he\u2019s dead.",
    "speaker": "General Sorrengail",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "cold",
