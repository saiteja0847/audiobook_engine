    "emotion_prompt": "sarcastic"
  },
  {
    "chunk_id": 31,
    "text": "[shouting] Yes!",
    "speaker": "General Sorrengail",
    "tts_config": {
      "provider": "mlx-cosyvoice3",
      "inference_method": "mlx",
      "speed": 1
    },
    "audio_effects": [],
    "metadata": {
      "type": "dialogue",
      "emotion": "angry",
